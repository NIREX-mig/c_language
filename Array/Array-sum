/*parpus:-Write a program in C

to find the sum of all elements

 of the array

author:- ashish kumar (NIreX)

date:-13/04/2021.

  */

#include<stdio.h>

void main()

{

    int arr[30];

    int n;

    int sum=0;

    printf("sum of all the elements of the array\n ");

    printf("_____________________________\n");

    printf("how many elements are store you :");

    scanf("%d",&n);

    

    for(int i=0; i<n; i++) //store the elements of array

    {

        printf("element %d :",i);

        scanf("%d",&arr[i]);

    }

    

    printf("\n\n");

    printf("sum of all elements stored in the array :");

    for(int i=0;i<n;i++)

    {

        sum +=arr[i];

    }

    printf("%d",sum);

}
